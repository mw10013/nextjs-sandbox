{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    },
    {
      "mode": "ts",
      "include": "**/*.ts",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    }
  ],
  "srcDir": "./db/",
  "camelCaseColumnNames": true,
  "hungarianNotation": false,
  "dbUrl": "postgresql://postgres:postgres@localhost:54322/postgres"
}
